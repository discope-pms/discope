{
    "name": "Guest List Item List",
    "description": "This view is intended for displaying the list of guest list items.",
    "access": {
        "groups": ["booking.default.user"]
    },
    "layout": {
        "items": [
            {
                "type": "field",
                "value": "firstname",
                "width": "20%"
            },
            {
                "type": "field",
                "value": "lastname",
                "width": "20%"
            },
            {
                "type": "field",
                "value": "is_coordinator",
                "width": "20%"
            },
            {
                "type": "field",
                "value": "gender",
                "width": "10%"
            },
            {
                "type": "field",
                "value": "date_of_birth",
                "width": "15%"
            },
            {
                "type": "field",
                "value": "citizen_identification",
                "width": "15%"
            },
            {
                "type": "field",
                "value": "address_street",
                "width": "20%"
            },
            {
                "type": "field",
                "value": "address_zip",
                "width": "10%"
            },
            {
                "type": "field",
                "value": "address_city",
                "width": "10%"
            },
            {
                "type": "field",
                "value": "address_country",
                "width": "10%"
            }
        ]
    }
}